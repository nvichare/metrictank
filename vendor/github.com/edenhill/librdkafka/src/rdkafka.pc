prefix=/usr/local
libdir=/usr/local/lib
includedir=/usr/local/include

Name: librdkafka
Description: The Apache Kafka C/C++ library
Version: 0.11.1
Cflags: -I${includedir}
Libs: -L${libdir} -lrdkafka
Libs.private: -lsasl2 -lrt -lcrypto -ldl -lz -lpthread -lssl -ldb -lgssglue -lsqlite3 -lpq -lgssapi -lmysqlclient -lldap
